<% if object.empty? %>
  <div class='alert alert-danger text-center px-2 py-4' role='alert'>
    <%= t('.not_found', name: params[:technology]) %>
  </div>
<% else %>
  <div class='alert alert-success alert-dismissible text-center px-2 py-4 mb-5' role='alert'>
    <button type='button' class='close' data-dismiss='alert' aria-label='Close'>
      <span aria-hidden="true">&times;</span>
    </button>
    <%= t('.found', unity: object.count, name: params[:technology]) %>
  </div>
<% end %>
